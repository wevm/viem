parseTransaction(&#x27;0x7ef83ca018040f35752170c3339ddcd850f185c9cc46bdef4d6e1f2ab323f4d3d710431994977f82a600a1414e583f7f13623f1ac5d58b1c0b808080808080&#x27;)\\n\",\"title\":\"Deposit Transactions\",\"titles\":[\"parseTransaction (OP Stack)\",\"Usage\"]},{\"href\":\"/op-stack/utilities/parseTransaction#returns\",\"html\":\"\\n<p><code>TransactionSerializable</code></p>\\n<p>The parsed transaction object.</p>\\n\",\"id\":\"pages/op-stack/utilities/parseTransaction.md#returns\",\"isPage\":false,\"text\":\"\\nTransactionSerializable\\nThe parsed transaction object.\\n\",\"title\":\"Returns\",\"titles\":[\"parseTransaction (OP Stack)\"]},{\"href\":\"/op-stack/utilities/parseTransaction#parameters\",\"html\":\"\\n\",\"id\":\"pages/op-stack/utilities/parseTransaction.md#parameters\",\"isPage\":false,\"text\":\"\\n\",\"title\":\"Parameters\",\"titles\":[\"parseTransaction (OP Stack)\"]},{\"href\":\"/op-stack/utilities/parseTransaction#serializedtransaction\",\"html\":\"\\n<ul>\\n<li><strong>Type:</strong> <code>Hex</code></li>\\n</ul>\\n<p>The serialized transaction.</p>\",\"id\":\"pages/op-stack/utilities/parseTransaction.md#serializedtransaction\",\"isPage\":false,\"text\":\"\\n\\nType: Hex\\n\\nThe serialized transaction.\",\"title\":\"serializedTransaction\",\"titles\":[\"parseTransaction (OP Stack)\",\"Parameters\"]}]}}"
