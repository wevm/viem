ePublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n// @filename: client.ts\\nimport { createPublicClient, http } from &#x27;viem&#x27;\\nimport { mainnet } from &#x27;viem/chains&#x27;\\n \\nexport const publicClient = createPublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n// @filename: client.ts\\nimport { createPublicClient, http } from &#x27;viem&#x27;\\nimport { mainnet } from &#x27;viem/chains&#x27;\\n \\nexport const publicClient = createPublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n// @filename: client.ts\\nimport { createPublicClient, http } from &#x27;viem&#x27;\\nimport { mainnet } from &#x27;viem/chains&#x27;\\n \\nexport const publicClient = createPublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n// @filename: example.js\\n \\n// ---cut---\\nimport { publicClient } from &#x27;./client&#x27;\\n \\nconst filter = await publicClient.createEventFilter(publicClient, {\\n  address: &#x27;0xa0b86991c6218b36c1d19d4a2e9eb0ce3606eb48&#x27;,\\n  event: { \\n    name: &#x27;Transfer&#x27;, \\n    inputs: [\\n      { type: &#x27;address&#x27;, indexed: true, name: &#x27;from&#x27; },\\n      { type: &#x27;address&#x27;, indexed: true, name: &#x27;to&#x27; },\\n      { type: &#x27;uint256&#x27;, indexed: false, name: &#x27;value&#x27; }\\n    ] \\n  }\\n})import { createPublicClient, http } from &#x27;viem&#x27;\\nimport { mainnet } from &#x27;viem/chains&#x27;\\n \\nexport const publicClient = createPublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n\",\"title\":\"Event\",\"titles\":[\"createEventFilter\",\"Scoping\"]},{\"href\":\"/docs/actions/public/createEventFilter#arguments\",\"html\":\"\\n<p>A Filter can be scoped to given <strong><em>indexed</em> arguments</strong>:</p>\\n<pre class=\\\"shiki shiki-themes github-light github-dark-dimmed has-focused\\\" style=\\\"background-color:#fff;--shiki-dark-bg:#22272e;color:#24292e;--shiki-dark:#adbac7\\\" tabindex=\\\"0\\\"><code><span class=\\\"l