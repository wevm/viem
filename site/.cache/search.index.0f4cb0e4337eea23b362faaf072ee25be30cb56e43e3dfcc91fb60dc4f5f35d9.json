uot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;ping&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n] as const\\n \\nexport const contractAddress = &#x27;0x...&#x27;\\n// @filename: contract.ts\\nexport const abi = [\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;initialize&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;ping&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n] as const\\n \\nexport const contractAddress = &#x27;0x...&#x27;\\n// @filename: contract.ts\\nexport const abi = [\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;initialize&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;ping&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n] as const\\n \\nexport const contractAddress = &#x27;0x...&#x27;\\n// @filename: contract.ts\\nexport const abi = [\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;initialize&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;ping&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n] as const\\n \\nexport const contractAddress = &#x27;0x...&#x27;\\n// @filename: config.ts\\nimport { createWalletClient, http } from &#x27;viem&#x27;\\nimport { sepolia } from &#x27;viem/chains&#x27;\\nimport { privateKeyToAccount } from &#x27;viem/accounts&#x27; \\n \\nexport const relay = privateKeyToAccount(&#x27;0x...&#x27;)\\n \\nexport const walletClient = createWalletClient({\\n  account: relay,\\n  chain: sepolia,\\n  transport: http(),\\n})\\n// @filename: example.js\\n \\n// ---cut---\\nimport { encodeFunctionData } from &#x27;viem&#x27;\\nimport { walletClient } from &#x27;./config&#x27;\\nimport { contractAddress } from &#x27;./contract&#x27;\\n \\nconst authorization = await walletClient.signAuthorization({\\n  account: eoa, \\n  contractAddress,\\n  executor: &#x27;self&#x27;, \\n})\\n \\nconst hash = await walletClient.sendTransaction({\\n  authorizationList: [authorization],\\n  data: encodeFunctionData({\\n    abi,\\n    functionName: &#x27;initialize&#x27;,\\n  }),\\n  to: eoa.address, \\n  to: walletClient.account.address, \\n})// @noErrors\\nimport { createWalletClient, http } from &#x27;viem&#x27;\\nimport { sepolia } from &#x27;viem/chains&#x27;\\nimport { privateKeyToAccount } from &#x27;viem/accounts&#x27;\\n \\nexport const relay = privateKeyToAccount(&#x27;0x...&#x27;) \\nexport const eoa = privateKeyToAccount(&#x27;0x...&#x27;) \\n \\nexport const walletClient = createWalletClient({\\n  account: relay, \\n  account: eoa, \\n  chain: sepolia,\\n  transport: http(),\\n})export const abi = [\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;initialize&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n  {\\n    &quot;type&quot;: &quot;function&quot;,\\n    &quot;name&quot;: &quot;ping&quot;,\\n    &quot;inputs&quot;: [],\\n    &quot;outputs&quot;: [],\\n    &quot;stateMutability&quot;: &quot;pure&quot;\\n  },\\n] as const\\n \\nexport const contractAddress = &#x27;0x...&#x27;\",\"title\":\"Note: Self-executing EIP-7702\",\"titles\":[\"Sending Transactions with EIP-7702\",\"Steps\"]}]}}"
