licClient = createPublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n// @filename: example.js\\n \\n// ---cut---\\nimport { account, publicClient } from &#x27;./config&#x27;\\n \\nconst result = await publicClient.createAccessList({ \\n  data: &#x27;0xdeadbeef&#x27;,\\n  to: &#x27;0x70997970c51812dc3a010c7d01b50e0d17dc79c8&#x27;\\n})import { createPublicClient, http } from &#x27;viem&#x27;\\nimport { mainnet } from &#x27;viem/chains&#x27;\\n \\nexport const publicClient = createPublicClient({\\n  chain: mainnet,\\n  transport: http()\\n})\\n\",\"title\":\"Usage\",\"titles\":[\"createAccessList\"]},{\"href\":\"/docs/actions/public/createAccessList#returns\",\"html\":\"\\n<p><code>{ accessList: AccessList, gasUsed: bigint }</code></p>\\n<p>The access list and gas used.</p>\\n\",\"id\":\"pages/docs/actions/public/createAccessList.md#returns\",\"isPage\":false,\"text\":\"\\n{ accessList: AccessList, gasUsed: bigint }\\nThe access list and gas used.\\n\",\"title\":\"Returns\",\"titles\":[\"createAccessList\"]},{\"href\":\"/docs/actions/public/createAccessList#parameters\",\"html\":\"\\n\",\"id\":\"pages/docs/actions/public/createAccessList.md#parameters\",\"isPage\":false,\"text\":\"\\n\",\"title\":\"Parameters\",\"titles\":[\"createAccessList\"]},{\"href\":\"/docs/actions/public/createAccessList#account-optional\",\"html\":\"\\n<ul>\\n<li><strong>Type:</strong> <code>Account | Address</code></li>\\n</ul>\\n<p>The Account to create an access list for.</p>\\n<pre class=\\\"shiki shiki-themes github-light github-dark-dimmed has-focused\\\" style=\\\"background-color:#fff;--shiki-dark-bg:#22272e;color:#24292e;--shiki-dark:#adbac7\\\" tabindex=\\\"0\\\"><code><span class=\\\"line\\\"><span style=\\\"color:#D73A49;--shiki-dark:#F47067\\\">import</span><span style=\\\"color:#24292E;--shiki-dark:#ADBAC7\\\"> { http, createPublicClient } </span><span style=\\\"color:#D73A49;--shiki-dark:#F47067\\\">from</span><span style=\\\"color:#032F62;--shiki-dark:#96D0FF\\\"> &