 @filename: example.js\\n \\n// ---cut---\\nimport { avalanche } from &#x27;viem/chains&#x27;\\nimport { walletClient } from &#x27;./client&#x27;\\n \\nawait walletClient.switchChain({ id: avalanche.id }) import &#x27;viem/window&#x27;\\n \\n// ---cut---\\nimport { createWalletClient, custom } from &#x27;viem&#x27;\\nimport { mainnet } from &#x27;viem/chains&#x27;\\n \\nexport const walletClient = createWalletClient({\\n  chain: mainnet,\\n  transport: custom(window.ethereum!),\\n})\\n\",\"title\":\"Usage\",\"titles\":[\"switchChain\"]},{\"href\":\"/docs/actions/wallet/switchChain#parameters\",\"html\":\"\\n\",\"id\":\"pages/docs/actions/wallet/switchChain.md#parameters\",\"isPage\":false,\"text\":\"\\n\",\"title\":\"Parameters\",\"titles\":[\"switchChain\"]},{\"href\":\"/docs/actions/wallet/switchChain#id\",\"html\":\"\\n<ul>\\n<li><strong>Type:</strong> <code>number</code></li>\\n</ul>\\n<p>The Chain ID.</p>\\n\",\"id\":\"pages/docs/actions/wallet/switchChain.md#id\",\"isPage\":false,\"text\":\"\\n\\nType: number\\n\\nThe Chain ID.\\n\",\"title\":\"id\",\"titles\":[\"switchChain\",\"Parameters\"]},{\"href\":\"/docs/actions/wallet/switchChain#json-rpc-methods\",\"html\":\"\\n<p><a href=\\\"https://eips.ethereum.org/EIPS/eip-3326\\\"><code>wallet_switchEthereumChain</code></a></p>\",\"id\":\"pages/docs/actions/wallet/switchChain.md#json-rpc-methods\",\"isPage\":false,\"text\":\"\\nwallet_switchEthereumChain\",\"title\":\"JSON-RPC Methods\",\"titles\":[\"switchChain\"]}]}}"
